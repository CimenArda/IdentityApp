
@{
    ViewData["Title"] = "Claims";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<ClaimViewModel>
<table class="table table-striped">
    <thead>
        <tr>
            <td>Dağıtan</td>
            <td>Type</td>
            <td>Value</td>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Issuer</td>
                <td>@item.Type</td>
                <td>@item.Value</td>
                
            </tr>
        }


    </tbody>

</table>
